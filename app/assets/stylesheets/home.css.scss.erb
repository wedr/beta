.nabizim{
    margin: 50px 0 0 0;
}
.nabizim h1 {
    margin: 0 0 20px 0;
}
.nabizim ul{
    height:90px;
}
.nabizim ul li{
    display:inline-block;
    height:90px;
    width:90px;
    margin: 0 35px;
}
.web {
    background: url(<%= asset_path 'web.png' %>);
}
.log {
    background: url(<%= asset_path 'log.png' %>);
}
.ui {
    background: url(<%= asset_path 'ui.png' %>);
}
.grafika {
    background: url(<%= asset_path 'grafika.png' %>);
}
.tisk {
    background: url(<%= asset_path 'tisk.png' %>);
}
.web:hover {
    background: url(<%= asset_path 'web.png' %>) 0 90px;
}
.log:hover  {
    background: url(<%= asset_path 'log.png' %>) 0 90px;
}
.ui:hover  {
    background: url(<%= asset_path 'ui.png' %>) 0 90px;
}
.grafika:hover  {
